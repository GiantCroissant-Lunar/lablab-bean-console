{
  "App": {
    "ApplicationName": "Lablab Bean Console",
    "Logging": {
      "EnableConsole": false,
      "EnableFile": true,
      "FilePath": "logs/lablab-bean-.log",
      "MinimumLevel": "Information"
    },
    "Version": "0.1.0"
  },
  "KernelMemory": {
    "Embedding": {
      "MaxTokens": 8191,
      "Provider": "OpenAI"
    },
    "Storage": {
      "ConnectionString": "",
      "Provider": "Volatile"
    },
    "TextGeneration": {
      "MaxTokens": 4096,
      "Provider": "OpenAI"
    }
  },
  "Plugins": {
    "HotReload": false,
    "Paths": [
      "plugins",
      "../../../plugins"
    ],
    "PreferredRenderer": "rendering-terminal",
    "PreferredUI": "ui-terminal",
    "Profile": "dotnet.console"
  },
  "Rendering": {
    "Terminal": {
      "PreferHighQuality": true,
      "TileSize": 16,
      "Tileset": "./assets/tiles.png"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "LablabBean.Plugins": "Information",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
          "path": "logs/lablab-bean-.log",
          "retainedFileCountLimit": 7,
          "rollingInterval": "Day"
        },
        "Name": "File"
      }
    ]
  },
  "VectorStore": {
    "File": {
      "Root": "data/vectorstore"
    },
    "Preferred": "File",
    "Qdrant": {
      "ApiKey": "",
      "BaseUrl": "http://localhost:6333",
      "Collection": "kb",
      "Dimension": 1536
    }
  }
}
