{
  "$schema": "../../framework/LablabBean.Plugins.Contracts/plugin.schema.json",
  "author": "Lablab Bean Team",
  "capabilities": [
    "video-decoding",
    "audio-decoding",
    "ffmpeg-integration",
    "frame-extraction"
  ],
  "configuration": {
    "autoStart": true,
    "enabled": true,
    "settings": {
      "ffmpegPath": "ffmpeg",
      "ffprobePath": "ffprobe"
    }
  },
  "dependencies": [
    {
      "id": "media-player-core"
    }
  ],
  "description": "FFmpeg-based media decoding engine for video and audio playback",
  "entryPoint": {
    "dotnet.console": "LablabBean.Plugins.MediaPlayer.FFmpeg.dll,LablabBean.Plugins.MediaPlayer.FFmpeg.FFmpegPlaybackPlugin"
  },
  "id": "media-player-ffmpeg",
  "name": "FFmpeg Media Engine",
  "permissions": {
    "profile": "Standard"
  },
  "priority": 60,
  "version": "1.0.0"
}
